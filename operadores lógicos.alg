algoritmo "semnome"
// Função :
// Autor :
// Data : 8/19/2024
// Seção de Declarações 
var
    a, b, c : inteiro
inicio
// Seção de Comandos 

a <- 2
b <- 3
c <- 5
escreval(a<b)
escreval(a>b)
escreval(a<>b)
escreval(a=b)
escreval(c<=a+b)
escreval((a<b) ou (c<b))
escreva((a=b) ou (c>a) e (c>b))
fimalgoritmo
